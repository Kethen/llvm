// llvm-profdata command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "llvm-profdata",
    defaults: ["llvm60-defaults"],
    srcs: ["llvm-profdata.cpp"],
    static_libs: [
        "libLLVM60Object",
        "libLLVM60ProfileData",
        "libLLVM60Core",
        "libLLVM60Support",
    ],
}
