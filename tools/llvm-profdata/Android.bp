// llvm-profdata command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "llvm-profdata",
    defaults: ["llvm50-defaults"],
    srcs: ["llvm-profdata.cpp"],
    static_libs: [
        "libLLVM50Object",
        "libLLVM50ProfileData",
        "libLLVM50Core",
        "libLLVM50Support",
    ],
}
