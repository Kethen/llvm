//===---------------------------------------------------------------===
// yaml2obj command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "yaml2obj",
    defaults: ["llvm60-defaults"],
    srcs: ["*.cpp"],
    static_libs: [
        "libLLVM60Object",
        "libLLVM60ObjectYAML",
        "libLLVM60MC",
        "libLLVM60MCParser",
        "libLLVM60BitReader",
        "libLLVM60Core",
        "libLLVM60Support",
    ],
}
