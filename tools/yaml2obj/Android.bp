//===---------------------------------------------------------------===
// yaml2obj command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "yaml2obj",
    defaults: ["llvm50-defaults"],
    srcs: ["*.cpp"],
    static_libs: [
        "libLLVM50Object",
        "libLLVM50ObjectYAML",
        "libLLVM50MC",
        "libLLVM50MCParser",
        "libLLVM50BitReader",
        "libLLVM50Core",
        "libLLVM50Support",
    ],
}
