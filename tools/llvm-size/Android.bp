//===---------------------------------------------------------------===
// llvm-size command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "llvm-size",
    defaults: ["llvm50-defaults"],
    srcs: ["llvm-size.cpp"],
    static_libs: [
        "libLLVM50Object",
        "libLLVM50MC",
        "libLLVM50MCParser",
        "libLLVM50BitReader",
        "libLLVM50Core",
        "libLLVM50Support",
    ],
}
