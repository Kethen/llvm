//===---------------------------------------------------------------===
// llvm-size command line tool
//===---------------------------------------------------------------===

cc_binary_host {
    name: "llvm-size",
    defaults: ["llvm60-defaults"],
    srcs: ["llvm-size.cpp"],
    static_libs: [
        "libLLVM60Object",
        "libLLVM60MC",
        "libLLVM60MCParser",
        "libLLVM60BitReader",
        "libLLVM60Core",
        "libLLVM60Support",
    ],
}
